{
  "name": "@hashgraph/hedera-wallet-connect",
  "version": "1.5.1",
  "description": "A library to facilitate integrating Hedera with Reown",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hashgraph/hedera-wallet-connect.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Hgraph <support@hgraph.com>",
  "keywords": [
    "hedera",
    "walletconnect",
    "reown",
    "hashgraph",
    "evm",
    "ethereum",
    "smart contracts",
    "hbar",
    "ethers"
  ],
  "license": "Apache-2.0",
